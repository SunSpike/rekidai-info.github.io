{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es2020",
    "lib": [
      "es2020",
      "DOM"
    ],
    "target": "es2020",
    "importsNotUsedAsValues": "error",
    "preserveValueImports": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "$lib": [
        "src/lib"
      ],
      "$lib/*": [
        "src/lib/*"
      ]
    },
    "rootDirs": [
      ".",
      ".svelte-kit/types"
    ]
  },
  "include": [
    "./src/**/*.js",
    "./src/**/*.ts",
    "./src/**/*.svelte"
  ],
  "exclude": [
    "./node_modules/**",
    "./svelte-kit/**",
    "./outDir/**/*"
  ]
}
